dsmax <- cpi %>%
mutate(color = colorize_vector(cpi_av, "A"),
y = cpi_max - cpi_min) %>%
select(x = country,
y,
name = country,
color,
mean = cpi_av,
max = cpi_max,
min = cpi_min) %>%
list.parse3()
dsmin <- cpi %>%
select(x = country, y = cpi_min) %>%
list.parse3()
# Some tooltips to make it a little *intercative*
x <- c("Min", "Mean", "Max")
y <- sprintf("{point.%s}", tolower(x))
tltip <- tooltip_table(x, y)
hc <- highchart() %>%
hc_chart(
type = "column",
polar = TRUE
) %>%
hc_plotOptions(
series = list(
stacking = "normal",
showInLegend = FALSE
)
) %>%
hc_xAxis(
categories = cpi$country
) %>%
hc_yAxis(
max = 15,
min = -5,
labels = list(format = "{value} C"),
showFirstLabel = FALSE
) %>%
hc_add_series(
data = dsmax
) %>%
hc_add_series(
data = dsmin,
color = "transparent",
enableMouseTracking = FALSE
) %>%
hc_add_theme(
hc_theme_smpl()
) %>%
hc_tooltip(
useHTML = TRUE,
pointFormat = tltip
)
hc
